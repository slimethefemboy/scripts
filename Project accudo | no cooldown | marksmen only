-- this script only works with marksmen class 
-- you can try to use other classes but
local Character = game.Players.LocalPlayer.Character
local c = game.Workspace.Camera
local e = game.StarterPlayer.StarterCharacterScripts

c:ClearAllChildren()
for i,v in pairs(Character:GetChildren()) do
if v:IsA("Script") then
  v:Clone()
   v:Destroy()
 end
end

for i,v in pairs(e:GetChildren()) do
if v.Name == "SecurityHandler" or v.Name == "SelfSecurity" then
   v:Destroy()
 end
end

for i,v in pairs(Character.Stats:GetChildren()) do
if v.Name == "CDRate" then
 v.Value = 0
end
end
